        <mxCell id="X7BS_atAVVZSwV2Onjna-58" value="âœ… POSITIVE DESIGN PATTERNS" style="shape=note;whiteSpace=wrap;html=1;backgroundOutline=1;fillColor=#e6ffe6;strokeColor=#00CC00;align=left;verticalAlign=top;size=20;fontStyle=1;fontSize=12;" vertex="1" parent="1">
